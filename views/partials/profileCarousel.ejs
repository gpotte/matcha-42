<div class="row">
  <div class="col-xs-3"></div>
<% if (profile.photo && profile.photo.length > 0) { %>
<div id="userCarousel" class="carousel slide col-xs-6" data-ride="carousel">
  <ol class="carousel-indicators">
    <%for (var i = 0; i < profile.photo.length; i++) {%>
    <li data-target"#userCarousel" data-slide-to="<%=i%>" <%if (i === 0){%> class=active <%}%>></li>
    <%}%>
  </ol>

  <!-- Wrapper for slides -->

  <div class="carousel-inner">
    <% for (i in profile.photo) { %>
      <% if (i == 0) {%>
        <div class="item active" data-toggle="modal" data-target="#photoModal<%=i%>"><img id="firstProfilePic" src="<%=profile.photo[i]%>"></img></div>
      <% } else {%>
        <div class="item" data-toggle="modal" data-target="#photoModal<%=i%>"><img src="<%=profile.photo[i]%>"></img></div>
      <%}%>
      <% include photoModal%>
      <%}%>
  </div>
  <!-- Left and right controls -->
  <a class="left carousel-control" href="#userCarousel" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#userCarousel" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
<%}%>
<div class="col-xs-3"></div>
</div>
